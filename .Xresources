! Font ----------------------------------------------

Xft*dpi:       96
Xft*antialias: true
Xft*rgba:      rgb
Xft*autohint:  false
Xft*lcdfilter: lcddefault
Xft*hinting:   true
Xft*hintstyle: hintslight

URxvt*letterSpace:    0
URxvt*font:           xft:JetBrainsMono Nerd Font Mono:style=Regular:pixelsize=16:antialias=true:hinting=true, xft:NotoMono Nerd Font:style=Book:antialias=true:hinting=true, xft:Hack Nerd Font:style=Regular:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=16, xft:Noto Color Emoji:style=Regular:pixelsize=16
URxvt*boldFont:       xft:JetBrainsMono Nerd Font Mono:style=Bold:pixelsize=16:antialias=true:hinting=true, xft:NotoMono Nerd Font:style=Book:antialias=true:hinting=true, xft:Hack Nerd Font Mono:style=Bold:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=16, xft:Noto Color Emoji:style=Regular:pixelsize=16
URxvt*italicFont:     xft:JetBrainsMono Nerd Font Mono:style=Italic:pixelsize=16:antialias=true:hinting=true, xft:NotoMono Nerd Font:style=Book:antialias=true:hinting=true, xft:Hack Nerd Font Mono:style=Italic:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=16, xft:Noto Color Emoji:style=Regular:pixelsize=16
URxvt*bolditalicFont: xft:JetBrainsMono Nerd Font Mono:style=Bold Italic:pixelsize=16:antialias=true:hinting=true, xft:NotoMono Nerd Font:style=Book:antialias=true:hinting=true, xft:Hack Nerd Font Mono:style=Bold Italic:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=16, xft:Noto Color Emoji:style=Regular:pixelsize=16

!URxvt*letterSpace:    0
!URxvt*font:           xft:Inconsolata Nerd Font:style=Regular:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=18:antialias=true:hinting=true, xft:Hack Nerd Font:style=Regular:pixelsize=18:antialias=true:hinting=true
!URxvt*boldFont:       xft:Inconsolata Nerd Font:style=Bold:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=18:antialias=true:hinting=true, xft:Hack Nerd Font:style=Bold:pixelsize=18:antialias=true:hinting=true
!URxvt*italicFont:     xft:Inconsolata Nerd Font:style=Italic:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=18:antialias=true:hinting=true, xft:Hack Nerd Font:style=Italic:pixelsize=18:antialias=true:hinting=true
!URxvt*bolditalicFont: xft:Inconsolata Nerd Font:style=Bold Italic:pixelsize=16:antialias=true:hinting=true, xft:Font Awesome:pixelsize=18:antialias=true:hinting=true, xft:Hack Nerd Font:style=Bold Italic:pixelsize=18:antialias=true:hinting=true

! Color -------------------------------------------

URxvt*.foreground: #ffffff
URxvt*.background: #222222
URxvt*.cursorColor: #ffffff

! black
*.color0: #000000
*.color8: #323232

! red
*.color1:  #ff4242
*.color9:  #f56262

! green
*.color2:  #7fe173
*.color10: #9dff91

! yellow
*.color3:  #ffc66d
*.color11: #ffe48b

! blue
*.color4:  #4099ff
*.color12: #5eb7f7

! magenta
*.color5:  #f680ff
*.color13: #ff9dff

! cyan
*.color6:  #85ffd8
*.color14: #c1f7e8

! white
*.color7:  #eeeeec
*.color15: #ffffff

! Settings -----------------------------------

URxvt*iconFile:             /usr/share/icons/Papirus/32x32/apps/Terminal.svg
!URxvt*backgroundPixmap:     /home/josh/.urxvt/borders/diamond.jpeg
URxvt*windowName:           urxvt
URxvt*windowTitle:          urxvt
URxvt*termName:             rxvt-256color
!URxvt*borderLess:           true
!URxvt*internalBorder:       10
!URxvt*externalBorder:       5
!URxvt*geometry:             136x38
URxvt*skipBuiltinGlyphs:    false
URxvt*scrollTtyKeypress:    true
URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollstyle:          plain
URxvt*scrollBar:            floating
URxvt*urgentOnBell:         true
URxvt*visualBell:           true
URxvt*saveLines:            10000
URxvt*mouseWheelScrollPage: false
URxvt*cursorBlink:          true
!URxvt*inheritPixmap:        false
URxvt*transparent:          false
URxvt*shading:              0
URxvt*depth:                32
URxvt*iso14755:             false
URxvt*iso14755_52:          false

! Extensions [/usr/lib/urxvt/perl]
URxvt*perl-ext-common: default,clipboard,keyboard-select,url-select

! Clipboard [Ctr+Shift+C/V]
!URxvt*clipboard.autocopy: true
!URxvt*keysym.C-S-C:       perl:clipboard:copy
!URxvt*keysym.C-S-V:       perl:clipboard:paste

! Clipboard [Ctrl+Shift+C/V]
URxvt*clipboard.copycmd:  xclip -i -selection clipboard
URxvt*clipboard.pastecmd: xclip -o -selection clipboard
URxvt*keysym.C-S-C:       perl:clipboard:copy
URxvt*keysym.C-S-V:       perl:clipboard:paste

! Keyboard-select
URxvt*keysym.C-Escape: perl:keyboard-select:activate
URxvt*keysym.C-S-S:    perl:keyboard-select:search

! Url-select
URxvt*keysym.C-S-U:         perl:url-select:select_next
URxvt*url-select.autocopy:  true
URxvt*url-select.button:    1
URxvt*url-select.launcher:  brave-bin
URxvt*url-select.underline: true
