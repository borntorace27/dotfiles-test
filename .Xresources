! Settings -----------------------------------

URxvt*iconFile:             /usr/share/icons/Fluentwin/scalable/apps/terminal.svg
!URxvt*backgroundPixmap:     /home/josh/.urxvt/borders/18.png
URxvt*windowName:           urxvt
!URxvt*borderLess:           true
!URxvt*internalBorder:       25
!URxvt*externalBorder:       5
!URxvt*skipBuiltinGlyphs:    true
URxvt*scrollstyle:          plain
URxvt*scrollBar:            false
URxvt*urgentOnBell:         true
URxvt*visualBell:           true
URxvt*saveLines:            10000
URxvt*mouseWheelScrollPage: false
URxvt*cursorBlink:          true
!URxvt*inheritPixmap:        false
URxvt*transparent:          true
URxvt*shading:              10
URxvt*depth:                32
URxvt*iso14755:             false
URxvt*iso14755_52:          false

! Extensions [~/.urxvt/ext]
URxvt*perl-ext-common:         default,clipboard,keyboard-select,url-select

URxvt*resize-font.step:        1
URxvt*keysym.Control-minus:    resize-font:smaller
URxvt*keysym.Control-plus:     resize-font:bigger
URxvt*keysym.Control-equal:    resize-font:reset
URxvt*keysym.Control-question: resize-font:show

! Clipboard [Ctr+Shift+C/V]
URxvt*keysym.Shift-Control-V: perl:clipboard:paste
URxvt*clipboard.autocopy:     true
URxvt*keysym.C-S-C:           perl:clipboard:copy
URxvt*keysym.C-S-V:           perl:clipboard:paste
URxvt*keysym.C-S-W:           perl:clipboard:paste_escaped

! Keyboard-select
URxvt*keysym.C-Escape: perl:keyboard-select:activate
URxvt*keysym.C-S-S:    perl:keyboard-select:search

! Url-select
URxvt*keysym.C-S-U:         perl:url-select:select_next
URxvt*url-select.autocopy:  true
URxvt*url-select.button:    1
URxvt*url-select.launcher:  /usr/bin/firefox
URxvt*url-select.underline: true
